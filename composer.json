{
  "name": "ivan-kuznetsov/docker-manager",
  "type": "library",
  "description": "Console manager for docker environment",
  "license": "MIT",
  "authors": [
    {
      "name": "Ivan Kuznetsov",
      "email": "myfortunemail@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "symfony/console": "^4.2",
    "symfony/yaml": "^4.2"
  },
  "bin": [
    "dm"
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app"
    },
    "exclude-from-classmap": [
      "/tests/"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}